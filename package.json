{
  "name": "manseeks",
  "version": "0.1.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron main.js",
    "electron-build": "ng build --prod && electron .",
    "package-windows": "electron-packager . manseeks-app --overwrite --asar.unpack='**/app/node_modules/rg-manseeks/bin/*/*' --platform=win32 --arch=ia32 --icon=src/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ManSeeks App\"",
    "package-mac": "electron-packager . --overwrite --platform=darwin --asar.unpack='**/app/node_modules/rg-manseeks/bin/*/*' --arch=x64 --icon=src/assets/icons/mac/manseeks-icon.png.icns --prune=true --out=release-builds",
    "package-linux": "electron-packager . manseeks --overwrite --asar.unpack='**/app/node_modules/rg-manseeks/bin/*/*' --platform=linux --arch=x64 --icon=src/assets/icons/png/512x512.png --prune=true --out=release-builds"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.0.3",
    "@angular/cdk": "^5.0.0-rc0",
    "@angular/common": "^5.0.3",
    "@angular/compiler": "^5.0.3",
    "@angular/core": "^5.0.3",
    "@angular/forms": "^5.0.3",
    "@angular/http": "^5.0.3",
    "@angular/material": "^5.0.0-rc0",
    "@angular/platform-browser": "^5.0.3",
    "@angular/platform-browser-dynamic": "^5.0.3",
    "@angular/router": "^5.0.3",
    "@swimlane/ngx-datatable": "^11.1.4",
    "app-root-dir": "^1.0.2",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "json2csv": "^3.11.5",
    "material-design-icons": "^3.0.1",
    "material-icons": "^0.1.0",
    "rg-manseeks": "^1.0.2",
    "rxjs": "^5.5.2",
    "text-encoding": "^0.6.4",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "15.0.4",
    "@angular/compiler-cli": "^5.0.3",
    "@angular/language-service": "^5.0.3",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^6.0.92",
    "codelyzer": "~3.2.0",
    "electron": "^1.8.8",
    "electron-packager": "^9.1.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
